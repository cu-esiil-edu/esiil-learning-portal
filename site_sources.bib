@article{quarderer_assessing_2022,
	title = {Assessing {Students}' {Self}-{Perceived} {Growth} in {Python} {Skills}, {Data} {Science} {Skills}, {Data} {Science} {Communication} {Skills}, {Science} {Practices} \& {Science} {Identity}: {An} {Examination} of the {Earth} {Data} {Science} {Corps} 2020-2021},
	shorttitle = {Assessing {Students}' {Self}-{Perceived} {Growth} in {Python} {Skills}, {Data} {Science} {Skills}, {Data} {Science} {Communication} {Skills}, {Science} {Practices} \& {Science} {Identity}},
	url = {https://doi.org/10.6084/m9.figshare.19633320.v1},
	doi = {10.6084/m9.figshare.19633320.v1},
	abstract = {Data-intensive skills are becoming increasingly critical in earth and environmental science. As data sets become larger, more numerous, and harder to analyze, there is a need for scientists who can develop earth and environmental data science (EDS) workflows that are open, reproducible, and use modern programming languages. To leverage the full potential of EDS, attention should also be focused on broadening participation by members of groups who have been historically underrepresented in data science and STEM. The Earth Data Science Corps (EDSC) program, hosted at Earth Lab (CIRES/CU Boulder) with partners from tribal colleges and minority-serving institutions throughout the Front Range and Northern Great Plains, offers one approach to introducing EDS to underserved communities. EDSC engages participants with fundamental Python programming tools and packages (pandas, numpy, matplotlib), along with resources to work with time- series, and vector and raster spatial data (earthpy, geopandas, rioxarray) in the context of EDS. Beginning in 2020, EDSC has built capacity for our partners to teach EDS at their home institutions and helped students develop confidence and comfort in a variety of different science and data science areas. Through a paid internship experience that includes a series of technical workshops and group projects, EDSC participants are assessed in the following five areas: (i) science identity, (ii) science practices, and (iii) Python, (iv) data science, and (v) science communication skills. Using a retrospective pre-then-post design, Likert-type survey items were analyzed using Rasch modeling to measure growth in EDSC participants’ self-reported comfort and confidence in the five areas outlined above. Analysis of variance (ANOVA) of person ability measures from Rasch modeling of survey responses collected during Year 1 of EDSC revealed significant differences from pre- to post- program, and between the five assessment areas. Post-hoc analysis pointed to significant growth in each of the five assessment areas with the exception of Science Identity. Collection and analysis of Year 2 EDSC survey responses is ongoing and results are forthcoming. Implications for teaching earth and environmental data science to members of historically underrepresented communities will be discussed.},
	language = {en},
	urldate = {2024-05-15},
	author = {Quarderer, Nathan and Wasser, Leah A. and Herwehe, Lauren and Montaño, Patricia and Gold, Anne U. and Korinek, Nathan},
	month = apr,
	year = {2022},
	note = {Publisher: figshare},
}

