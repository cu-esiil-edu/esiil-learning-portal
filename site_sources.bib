@article{quarderer_fostering_2024,
	title = {Fostering the {Development} of {Earth} {Data} {Science} {Skills} in a {Diverse} {Community} of {Online} {Learners}: {A} {Case} {Study} of the {Earth} {Data} {Science} {Corps}},
	shorttitle = {Fostering the {Development} of {Earth} {Data} {Science} {Skills} in a {Diverse} {Community} of {Online} {Learners}},
	url = {https://osf.io/w6crb},
	doi = {10.31219/osf.io/w6crb},
	abstract = {Today’s data-driven world requires earth and environmental scientists to have skills at the intersection of domain and data science. These skills are imperative to harness information contained in a growing volume of complex data to solve the world's most pressing environmental challenges. Despite the importance of these skills, Earth and Environmental Data Science (EDS) training is not equally accessible, contributing to a lack of diversity in the field. This creates a critical need for EDS training opportunities designed specifically for underrepresented groups. In response, we designed the Earth Data Science Corps (EDSC) which couples a paid internship for undergraduate students with faculty training to build capacity to teach and learn EDS using Python at smaller Minority Serving Institutions. EDSC participants are further empowered to teach these skills at their home institutions which scales the program beyond the training lead by our team. Using a Rasch modeling approach, we found that participating in the EDSC program had a significant impact on learners’ comfort and confidence with technical and non-technical data science skills, as well as their science identity and sense of belonging in science, two critical aspects of recruiting and retaining members of underrepresented groups in STEM.},
	language = {en-us},
	urldate = {2024-01-05},
	journal = {OSF Preprints},
	author = {Quarderer, Nathan and Wasser, Leah and Herwehe, Lauren and Gold, Anne U. and Montaño, Patricia A. and Halama, Katherine and Biggane, Emily and Logan, Jessica and Parr, David and Brady, Sylvia and Tinant, Charles Jason and Ward, Emily and Thunder, Elisha Yellow and Eyes, Justina White and Bear/Bagola, LaShell Poor and Phelps, Madison and Phelps, Trey Orion and Johnson, Michela and Jacquez, Naomi and Rohlehr, Kaiea and Alberts, Brett and Sanovia, James and Culler, Elsa and Nagy, Chelsea and Balch, Jennifer},
	month = jan,
	year = {2024},
	note = {Publisher: OSF},
}

