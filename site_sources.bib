@misc{culler_open_2024,
	address = {Long Beach, CA, USA},
	type = {Symposium},
	title = {Open for whom? {Where} we stand with inclusive open science},
	url = {https://events.rdmobile.com/Sessions/Details/2381994},
	abstract = {The Budapest Open Access Initiative Declaration from 2002 states that “Removing access barriers to this literature will accelerate research, enrich education, share the learning of the rich with the poor and the poor with the rich, make this literature as useful as it can be, and lay the foundation for uniting humanity in a common intellectual conversation and quest for knowledge.”  In the past twenty years, we have seen a massive shift to open access publishing. Major governmental organizations, such as the US National Institutes of Health, now require their funded research to be published openly, and major open-access publishers like PLOS are self-sustaining. Furthermore, it has become much more common for journals to suggest or require data and analysis code to be published alongside academic papers. For example, Copernicus Publications “...requests depositing data that correspond to journal articles in reliable (public) data repositories, assigning digital object identifiers, and properly citing data sets as individual contributions.” The FAIR (Findable, Accessible, Interoperable, Reusable) open data principles are also cited by many publishers, including ESA, in their data policies. Notwithstanding these victories for open science, it is important to consider to what degree the goals of open science have been realized in the past two decades. Open Access journals often shift costs from readers to researchers, making it nearly impossible for researchers from poorer countries or institutions to access peer reviewed open publication. The Global Indigenous Data Alliance responded to the FAIR principles with the CARE (Collective Benefit, Authority to control, Responsibility, Ethics) principles, which advocate against the indiscriminate release of sensitive knowledge and data. Finally, writers such as Catherine D’Ignacio and Lauren Klein in their book Data Feminism point out several other ways in which the reality of open science falls short of goals of equity and justice. In this presentation, we will discuss the goals of Open Access Knowledge, and identify both the progress that has been made and the issues that still need to be addressed.},
	author = {Culler, Elsa},
	month = aug,
	year = {2024},
}

