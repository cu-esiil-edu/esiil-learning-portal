
@misc{balch_cos_2023,
	address = {Portland, OR, USA},
	type = {Contributed {Talk}},
	title = {{COS} 208-5 - {The} {Environmental} {Data} {Science} {Innovation} \& {Inclusion} {Lab} ({ESIIL}): a next-generation {NSF} data synthesis center ({Version} 1).},
	url = {https://doi.org/10.6084/m9.figshare.24263068.v1},
	abstract = {The Environmental Data Science Innovation \& Inclusion Lab (ESIIL) is a next-generation NSF synthesis center led by the University of Colorado Boulder in collaboration with NSF’s CyVerse at the University of Arizona and the University of Oslo. ESIIL enables a global community of environmental data scientists to leverage the wealth of environmental data and emerging analytics to develop science-based solutions to solve pressing challenges in biology and other environmental sciences. ESIIL holds inclusion as a core principle and method for diversifying environmental data science at a time when society needs all perspectives, and science needs to serve all. ESIIL’s research community generates discoveries and novel approaches through: 1) cutting-edge team science, 2) innovative tools and collaborative cyberinfrastructure, 3) data science education and training, and 4) inclusive participation and diverse groups. These activities advance the frontier of environmental data science, a rapidly evolving discipline bridging the computational, biological, environmental, and social sciences. ESIIL’s open Collaborative and Scalable Environment (CASE) cyberinfrastructure lowers barriers to scientific collaboration through tailored user experience, seamless connection to critical data sources, and premier cloud computing. ESIIL’s education program facilitates broad access to environmental data science skills and helps develop the next-generation data-capable workforce through our online learning portal, reaching over two million unique users each year. Partnerships with Tribal Colleges and Minority-Serving Institutions increase data skills and knowledge sharing among underrepresented groups. Join us in building the ESIIL Network, a diverse and inclusive community that is committed to generating data-inspired discoveries that enhance societal and ecosystem resilience.},
	author = {Balch, Jennifer K. and Nagy, R. Chelsea and Hummel do Amaral, Cibele and Culler, Elsa and Gold, Anne and Iglesias, Virginia and Monteleoni, Claire and Parker, John N. and Rattling Leaf, James and Sanovia, Jim and Stavros, Natasha E. and Sullivan, Susan and Swetnam, Tyson and Tuff, Ty and Quarderer, Nathan},
	month = aug,
	year = {2023},
}

@misc{quarderer_creating_2023,
	address = {Portland, OR, USA},
	type = {Poster {Presentation}},
	title = {Creating inclusive spaces for environmental data science education.},
	url = {https://doi.org/10.6084/m9.figshare.23990001.v2},
	abstract = {As the ecological and environmental sciences become more data intensive, there are growing demands for training designed to help early-career scientists level-up their technical data and analytics skills. The paths to acquiring the interdisciplinary sets of skills required to work with increasingly large and complex environmental data are frequently filled with a variety of obstacles, particularly for members from historically marginalized groups. We present models for Environmental Data Science (EDS) education that help meet the need for data-intensive training for life, earth, and environmental scientists from a diversity of backgrounds and career stages, including skills related to remote sensing, coding, and programming. Led by researchers at Earth Lab and the NSF-funded Environmental Data Science Innovation \& Inclusion Lab (ESIIL), at the University of Colorado Boulder, this study will present findings from surveys of students enrolled in the Earth Data Analytics Professional Graduate Certificate Program (EDA), and the NSF-funded Earth Data Science Corps (EDSC). Over five years, these two programs have helped a diverse cohort of professionals (n = 96) gain fundamental EDS skills for career advancement, and built capacity to teach and learn EDS at other institutions catering to minority communities, including Tribal Colleges and Universities and Hispanic Serving Institutions. Students enrolled in these programs learn how to find and work with data to answer questions in the context of applied ecological and environmental science using the Python programming language. Students then apply their new skills by designing and completing culturally-relevant projects that have focused on topics including characterizing vegetation using NEON remote sensing data, emerald ash borer detections in Minnesota cities, bird behaviors within the Prairie Pothole Region, and more. All lessons are taught using a free and open curriculum, which provides widely accessible teaching and learning materials (earthdatascience.org). Using Likert surveys and a Rasch modeling approach, we demonstrate that students from these programs show significant growth in their self-reported comfort and confidence with different data science skills (p {\textless} 0.001), as well as a deeper understanding of core disciplinary ecological and environmental science concepts. In the coming months, we will launch the ESIIL Stars internship and ESIIL Data Short Course, modeled after Earth Lab’s EDSC and EDA programs. Inclusive programs such as these are integral to innovation in data-intensive education and empowering the next generation of diverse ecological and environmental data scientists.},
	language = {English},
	author = {Quarderer, Nathan and Halama, Katherine and Post, Alison and Hummel do Amaral, Cibele and Culler, Elsa and Nagy, R. Chelsea and Tuff, Ty and Sanovia, Jim and Balch, Jennifer K.},
	month = aug,
	year = {2023},
}

